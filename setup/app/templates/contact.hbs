<div class="container-fluid ">
    <div class="row">
        <div class="col-md-6">
            {{#each model as |teammember|}}
                <div class="row">
                    <h4>Name</h4>
                    <p>{{teammember.name}}</p>
                </div>
                <div class="row">
                    <h4>GitHub Username</h4>
                    <p>{{teammember.github}}</p>
                </div>
                <div class="row">
                    <h4>Email</h4>
                    <p>{{teammember.email}}</p>
                </div>
            {{/each}}
        </div>
    </div>
</div>
